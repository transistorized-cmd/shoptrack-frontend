{
  "name": "shoptrack-frontend",
  "version": "0.0.1",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=22.0.0",
    "npm": ">=10.0.0"
  },
  "scripts": {
    "preinstall": "node scripts/check-node-version.cjs",
    "dev": "node scripts/check-node-version.cjs && vite",
    "build": "node scripts/check-node-version.cjs && run-p type-check \"build-only {@}\" --",
    "build:analyze": "node scripts/check-node-version.cjs && ANALYZE=true npm run build",
    "build:stats": "node scripts/check-node-version.cjs && ANALYZE=true vite build --mode production",
    "preview": "node scripts/check-node-version.cjs && vite preview",
    "build-only": "node scripts/check-node-version.cjs && vite build",
    "type-check": "node scripts/check-node-version.cjs && vue-tsc --build --force",
    "lint": "eslint . --fix",
    "lint:check": "eslint .",
    "format": "prettier --write src/",
    "analyze": "npm run build:analyze",
    "bundle:analyze": "node scripts/bundle-analysis.js",
    "test": "node scripts/check-node-version.cjs && vitest",
    "test:ui": "node scripts/check-node-version.cjs && vitest --ui",
    "test:run": "node scripts/check-node-version.cjs && vitest run",
    "test:coverage": "node scripts/check-node-version.cjs && vitest run --coverage",
    "test:performance": "./scripts/run-performance-tests.sh",
    "test:performance:components": "./scripts/run-performance-tests.sh -m components",
    "test:performance:stores": "./scripts/run-performance-tests.sh -m stores",
    "test:performance:api": "./scripts/run-performance-tests.sh -m api",
    "test:performance:ci": "./scripts/run-performance-tests.sh -e ci",
    "test:unit": "TEST_CATEGORIES=unit npm run test:run",
    "test:integration": "TEST_CATEGORIES=integration npm run test:run",
    "test:components": "TEST_CATEGORIES=component npm run test:run",
    "test:views": "TEST_CATEGORIES=view npm run test:run",
    "test:stores": "TEST_CATEGORIES=store npm run test:run",
    "test:services": "TEST_CATEGORIES=service npm run test:run",
    "test:fast": "TEST_CATEGORIES=fast npm run test:run",
    "test:medium": "TEST_CATEGORIES=medium npm run test:run",
    "test:slow": "TEST_CATEGORIES=slow npm run test:run",
    "test:stable": "TEST_CATEGORIES=stable npm run test:run",
    "test:critical": "TEST_CATEGORIES=critical npm run test:run",
    "test:auth": "TEST_CATEGORIES=auth npm run test:run",
    "test:receipts": "TEST_CATEGORIES=receipts npm run test:run",
    "test:reports": "TEST_CATEGORIES=reports npm run test:run",
    "test:plugins": "TEST_CATEGORIES=plugins npm run test:run",
    "test:shallow": "TEST_CATEGORIES=shallow npm run test:run",
    "test:full-mount": "TEST_CATEGORIES=full-mount npm run test:run",
    "test:pre-commit": "TEST_CATEGORIES=pre-commit npm run test:run",
    "test:smoke": "TEST_CATEGORIES=smoke npm run test:run",
    "test:exclude-flaky": "EXCLUDE_CATEGORIES=flaky npm run test:run",
    "test:exclude-slow": "EXCLUDE_CATEGORIES=slow npm run test:run",
    "test:ci-fast": "TEST_CATEGORIES=unit,fast,stable npm run test:run",
    "test:ci-full": "EXCLUDE_CATEGORIES=flaky,experimental npm run test:run",
    "with-nvm": "./scripts/with-nvm.sh",
    "nvm:test": "./scripts/with-nvm.sh npm run test",
    "nvm:test:run": "./scripts/with-nvm.sh npm run test:run",
    "nvm:dev": "./scripts/with-nvm.sh npm run dev",
    "nvm:build": "./scripts/with-nvm.sh npm run build"
  },
  "dependencies": {
    "@stripe/stripe-js": "^7.9.0",
    "@vueuse/core": "^11.2.0",
    "axios": "^1.7.7",
    "chart.js": "^4.4.6",
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-chartjs": "^5.3.1",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.4.5"
  },
  "devDependencies": {
    "@axe-core/playwright": "^4.10.2",
    "@faker-js/faker": "^10.0.0",
    "@intlify/unplugin-vue-i18n": "^11.0.1",
    "@playwright/test": "^1.55.0",
    "@rushstack/eslint-patch": "^1.10.4",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/user-event": "^14.6.1",
    "@testing-library/vue": "^8.1.0",
    "@tsconfig/node20": "^20.1.4",
    "@types/node": "^20.17.6",
    "@typescript-eslint/eslint-plugin": "^8.42.0",
    "@typescript-eslint/parser": "^8.42.0",
    "@vitejs/plugin-vue": "^5.1.4",
    "@vitest/coverage-v8": "^3.2.4",
    "@vitest/ui": "^3.2.4",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/eslint-config-typescript": "^14.1.3",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.5.1",
    "autoprefixer": "^10.4.20",
    "eslint": "^9.13.0",
    "eslint-plugin-vue": "^9.29.1",
    "happy-dom": "^18.0.1",
    "jest-axe": "^10.0.0",
    "msw": "^2.11.1",
    "npm-run-all2": "^7.0.1",
    "postcss": "^8.4.49",
    "prettier": "^3.3.3",
    "rollup-plugin-visualizer": "^6.0.3",
    "structured-clone": "^0.2.2",
    "tailwindcss": "^3.4.14",
    "terser": "^5.44.0",
    "typescript": "~5.6.2",
    "vite": "^7.1.4",
    "vite-bundle-analyzer": "^1.2.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.1.6"
  }
}
